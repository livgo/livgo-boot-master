    <properties>
        <livgo.core.version>1.0.0</livgo.core.version>
        <livgo.common.version>1.0.0</livgo.common.version>
        <livgo.model.version>1.0.0</livgo.model.version>
        <livgo.data.version>1.0.0</livgo.data.version>
